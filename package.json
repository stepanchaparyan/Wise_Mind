{
  "name": "wise_mind",
  "version": "1.0.0",
  "description": "Medical",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --mode production",
    "dev": "rimraf build && cross-env NODE_ENV=development webpack --mode development",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "test": "jest --config ./jest.config.js",
    "test:update": "jest --verbose --coverage --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stepanchaparyan/Wise_Mind.git"
  },
  "keywords": [
    "react",
    "medical"
  ],
  "author": "Stepan Chaparyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stepanchaparyan/Wise_Mind/issues"
  },
  "homepage": "https://github.com/stepanchaparyan/Wise_Mind#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{json|css|scss|md}": [
      "prettier --write",
      "git add"
    ]
